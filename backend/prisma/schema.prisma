generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ImageUpload {
  id           Int      @id @default(autoincrement())
  filename     String
  uploadedAt   DateTime @default(now())
  result       String?  // JSON หรือข้อความผลวิเคราะห์จากโมเดล
  confidence   Float?   // ความมั่นใจของโมเดล (ถ้ามี)
}

